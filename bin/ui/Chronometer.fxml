<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChronometerController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="chronometer" layoutX="86.0" layoutY="92.0" text="AAAA" />
            <Button layoutX="5.0" layoutY="142.0" mnemonicParsing="false" onAction="#parar" text="Pausar" />
            <Button layoutX="61.0" layoutY="142.0" mnemonicParsing="false" onAction="#reiniciar" text="Reiniciar" />
            <Button layoutX="127.0" layoutY="142.0" mnemonicParsing="false" onAction="#continueP" text="Continuar" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
